file = {:Users/samuelmerk/Documents/Mendeley Desktop/Kluger, DeNisi - 1996 - The effects of feedback interventions on performance A historical review, a meta-analysis, and a preliminary fee.pdf:pdf},
isbn = {190393429X},
issn = {0033-2909},
journal = {Psychological Bulletin},
number = {2},
pages = {254--284},
