@unpublished{Porsch2010,
abstract = {Abstract: The growth of scientific and technological knowledge in modern societies has lead to an increase of specialization of knowledge and expertise. Most socio-scientific issues are far too complex to be understood deeply by laypersons. From various disciplines we have to choose pertinent ones if we want to rely on expert advice. Epistemological beliefs might be helpful to cope with this challenge. Furthermore it is necessary to have realistic awareness of one's own fragmentary understanding and to avoid the ``illusion of explanatory depth'' (Rosenblit {\{}{\&}{\}} Keil, 2002). In order to research on adults' capability to choose between disciplines who might be relevant for a science topic, N = 520 secondary school students were asked to choose, which of 22 scientific disciplines (e.g. math, geology, biology) should contribute to a book about tide and float. They were also asked to assess their own knowledge about the theme. Influence of epistemological beliefs has been tested by an epistemological sensitization in an experimental design. The epistemological sensitization significantly influences students' self-assessment of knowledge and discipline rating. Students with sophisticated epistemological beliefs were more critical about their own knowledge about tide and flow, chose significantly pertinent and -by tendency- potential pertinent disciplines more and declined non-pertinent disciplines more.},
author = {Porsch, Torsten and Bromme, Rainer},
file = {:Users/samuelmerk/Documents/Mendeley Desktop/Porsch, Bromme - 2010 - Which science disciplines are pertinent - Impact of epistemological beliefs on students' choices.pdf:pdf},
title = {{Which science disciplines are pertinent? - Impact of epistemological beliefs on students' choices}},
year = {2010}
}
