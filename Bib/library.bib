doi = {10.3389/fpsyg.2014.00748},
file = {:Users/samuelmerk/Documents/Mendeley Desktop/Kelava, Brandt - 2014 - A general non-linear multilevel structural equation mixture model.pdf:pdf},
issn = {16641078},
