doi = {10.1207/s15327809jls1502_4},
file = {:Users/samuelmerk/Documents/Mendeley Desktop/Rosenberg, Hammer, Phelan - 2006 - Multiple epistemological coherences in an eighth-grade discussion of the rock cycle.pdf:pdf},
