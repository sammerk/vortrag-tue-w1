@article{Widaman1993,
abstract = {Vgl. RID 10722: Argumentation f{\"{u}}r CFA statt PCA Conducted analyses to demonstrate the differences in population parameters (POPPs) represented by the principle component analysis (PCA) and common factor analysis (CFA) models. The analyses included (1) a replication of the S. C. Snook and R. L. Gorsuch (see record 1989-35232-001) study, using population data instead of simulated sample data and (2) an extension of the Snook and Gorsuch study designed to test their claim that differences in pattern loadings are an inverse function of the number of observed variables in an analysis. In contrast to the findings by W. F. Velicer and D. N. Jackson (see record 1990-27325-001) in this area, the present results suggest that there were systematic differences between POPPs defined by PCA and CFA and that these differences were present even when the correct number of dimensions was retained. Also, the POPPs defined by CFA may have been more generalizable than those defined by PCA. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
author = {Widaman, Keith F.},
doi = {10.1207/s15327906mbr2803_1},
