journal = {Journal of Teacher Education},
keywords = {achievement,assessment,development,general pedagogical knowledge,international education,studies,teacher education,teacher knowledge and beliefs},
number = {2},
pages = {188--201},
title = {{General Pedagogical Knowledge of Future Middle School Teachers: On the Complex Ecology of Teacher Education in the United States, Germany, and Taiwan}},
volume = {62},
