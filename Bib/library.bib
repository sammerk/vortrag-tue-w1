@article{Carlson1999,
abstract = {This study examines the role of social support in work--family conflict. Although previous research has examined social support as a promising coping mechanism, questions as to how social support affects work--family conflict remain unanswered. Participants were 314 adults (mean age 44 yrs) who were married or had children living at home. Models of role conflict, role ambiguity, time demands, social support, involvement and satisfaction, and work--family conflict were tested using structural equation modeling. Results suggest that social support may be best viewed as an antecedent to perceived stressors, and may reduce perceived role stressors (conflict and ambiguity) and time demands to indirectly decrease work--family conflict. Individuals who develop strong social support networks at work and at home may perceive fewer stressors in organizational and family life. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
