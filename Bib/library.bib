@article{Schafer2013,
abstract = {{\{}{\#}{\}}englisch{\{}{\#}{\}}Despite all strategic contrasting of the seemingly unequivocal categories of {\{}$\backslash$dq{\}}theory{\{}$\backslash$dq{\}} and {\{}$\backslash$dq{\}}empiricism{\{}$\backslash$dq{\}}, these still have one problem in common: namely that of representation. This does not only refer to the fact that linguistic symbolizations do not correspond to {\{}$\backslash$dq{\}}the one{\{}$\backslash$dq{\}} reality. The problem of representation manifests itself furthermore and above all in that a system of {\{}$\backslash$dq{\}}fixed{\{}$\backslash$dq{\}} meanings can be achieved neither through philosophical foundations nor through the belief in an empirical reality. A field of conflict emerges in which categorical and empirical applications are granted a strategic-political status. The author argues that such an approach will be able not only to mobilize the strategically hardened corrals of {\{}$\backslash$dq{\}}theory{\{}$\backslash$dq{\}} and {\{}$\backslash$dq{\}}empiricism{\{}$\backslash$dq{\}} in educational science, but also to redirect them onto a more productive path. (DIPF/Orig.).;;{\{}{\#}{\}}deutsch{\{}{\#}{\}}Bei aller strategischen Entgegensetzung der scheinbar eindeutigen Kategorien von {\{}$\backslash$dq{\}}Theorie{\{}$\backslash$dq{\}} und {\{}$\backslash$dq{\}}Empirie{\{}$\backslash$dq{\}} haben beide doch ein Problem gemeinsam: das der Repr{\{}{\"{a}}{\}}sentation. Damit ist nicht nur gemeint, dass sprachliche Symbolisierungen nicht {\{}$\backslash$dq{\}}der{\{}$\backslash$dq{\}} Wirklichkeit entsprechen. Das Problem der Repr{\{}{\"{a}}{\}}sentation zeigt sich vor allem und dar{\{}{\"{u}}{\}}ber hinaus darin, dass ein System {\{}$\backslash$dq{\}}fester{\{}$\backslash$dq{\}} Bedeutungen weder durch philosophische Begr{\{}{\"{u}}{\}}ndung noch durch den Glauben an eine empirische Wahrheit zu erreichen ist. Es zeichnet sich dann ein Feld von Auseinandersetzungen ab, in dem kategoriale und empirische Eins{\{}{\"{a}}{\}}tze einen strategisch-politischen Status erhalten. Es wird zu zeigen versucht, dass eine solche Sichtweise nicht nur die strategisch verfestigten Wagenburgen von {\{}$\backslash$dq{\}}Theorie{\{}$\backslash$dq{\}} und {\{}$\backslash$dq{\}}Empirie{\{}$\backslash$dq{\}} in der Erziehungswissenschaft in Bewegung zu bringen, sondern auf beiden {\{}$\backslash$dq{\}}Feldern{\{}$\backslash$dq{\}} auch produktiv zu wenden vermag. (DIPF/Orig.).},
author = {Sch{\"{a}}fer, Alfred},
issn = {0044-3247},
journal = {Zeitschrift f{\"{u}}r P{\"{a}}dagogik},
keywords = {Erziehungswissenschaft Empirische Forschung Diskur},
