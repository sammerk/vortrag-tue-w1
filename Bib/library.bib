@article{Klusmann2006c,
abstract = {The transition to work is an important phase in the process of teachers' professional development. However, there is evidence that not all teachers deal successfully with the demands of the transition resulting in high levels of occupational stress and burnout symptoms. The present study investigates first the development of emotional exhaustion and job satisfaction in the first two years of beginning teachers. Second, we investigate the effects of personality traits, previous educational experiences, pedagogical content knowledge, knowledge about classroom behavior and self-regulatory skills on student teachers emotional exhaustion and job satisfaction. The data consists of more than 500 beginning teachers in their practical phase of teacher education, who were examined in a multi-cohort longitudinal study. The results suggest that emotional exhaustion increases significantly in the first year of teaching practice, while it remains stable in the second year Compared to a sample of experienced teachers, beginning teachers report similar emotional exhaustion but higher job satisfaction. Personality traits as well as aspects of professional knowledge and educational experience explained interindividual differences in the development of exhaustion and satisfaction over one year. The implications of the present results for the professional development of teachers are discussed. (PsycINFO Database Record (c) 2013 APA, all rights reserved) (journal abstract)},
author = {Klusmann, Uta and Kunter, Mareike and Voss, Thamar and Baumert, J{\"{u}}rgen},
doi = {10.1024/1010-0652/a000078},
