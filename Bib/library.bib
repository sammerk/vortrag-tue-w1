number = {4},
pages = {306--318},
publisher = {Elsevier Inc.},
title = {{Epistemic beliefs: Setting the standards for self-regulated learning}},
url = {http://dx.doi.org/10.1016/j.cedpsych.2009.06.005 http://www.redi-bw.de/db/ebsco.php/search.ebscohost.com/login.aspx?direct=true{\&}db=psyh{\&}AN=2009-12555-006{\&}site=ehost-live},
